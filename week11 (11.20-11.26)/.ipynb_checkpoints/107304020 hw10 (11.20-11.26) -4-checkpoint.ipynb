   "source": [
    "a, b = input('請輸入二個正整數：').split()\n",
    "a = int(a)\n",
    "b = int(b)\n",
    "\n",
    "if a < b:\n",
    "    a, b = b, a\n",
    "\n",
    "r = a % b\n",
    "\n",
    "while r != 0:\n",
    "    a = b\n",
    "    b = r\n",
    "    r = a % b\n",
    "\n",
    "print('GCD是 ' + str(b))"
